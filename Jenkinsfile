pipeline {
   agent any

   stages {
      stage("build") {
        steps {
          echo 'building our app...'
        }
      }

      stage("test") {
        steps {
         echo 'testing our app...'
        }
      }

      stage("another test") {
        steps {
         echo 'testing our app again...'
        }
      }

      stage("Final test") {
        steps {
         echo 'testing the app finally...'
        }
      }
      
         stage("KIDDING") {
        steps {
         echo 'one more test!!'
        }
      }


      stage("deploy") {
        steps {
         echo 'deploying our app...'
        }
      }

   }
}
